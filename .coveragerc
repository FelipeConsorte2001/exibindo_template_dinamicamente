[run]
source = .
omit =
    */__init__.py
    */settings.py
    */manage.py
    */wsgi.py
    */apps.py
    */urls.py
    */admin.py
    */migrations/*
    */tests/*

[report]
omit =
    ./venv/*,
    *testes*,
    *apps.py,
    *manage.py,
    *__init__.py,
    *migrations*,
    *asgi*,
    *wsgi*,
    *admin.py,
    *urls.py,
